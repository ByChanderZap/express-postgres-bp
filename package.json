{
  "name": "express-postgres-bp",
  "version": "1.0.0",
  "description": "boilerplate of an api builded with typescript, postgresql and sequelize",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json src/main.ts",
    "dockerize": "docker-compose build dev && docker-compose up",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "format": "npm run lint:fix",
    "lint": "eslint ./src",
    "lint:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src",
    "lint:ci:check": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --quiet",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts,.tsx ./src --fix",
    "test": "jest --no-watch"
  },
  "keywords": [
    "boilerplate",
    "node",
    "sequelize",
    "docker",
    "postgresql",
    "typescript"
  ],
  "author": "Alexander Herrera | alexander.herrerain@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "eslint": "^8.46.0",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@types/basic-auth": "^1.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5",
    "standard": "^17.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
